{
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.starcyeed.com",
    "NEXT_PUBLIC_WS_URL": "wss://api.starcyeed.com"
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "starcyeed.com" }],
      "destination": "https://www.starcyeed.com/:path*",
      "permanent": true
    }
  ]
}