{
  "name": "chatter-box-frontend",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "https://web-production-3ba7e.up.railway.app",
    "NEXT_PUBLIC_WS_URL": "wss://web-production-3ba7e.up.railway.app"
  }
}